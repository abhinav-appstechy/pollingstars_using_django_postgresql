{
    "version": 2,
    "builds": [
      {
        "src": "./pollingproject/pollingproject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }

    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./pollingproject/pollingproject/wsgi.py"
      }
    ]
  }
  